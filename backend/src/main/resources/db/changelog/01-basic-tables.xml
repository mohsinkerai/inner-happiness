        <constraints nullable="true" unique="false"/>
      </column>
      <column name="updated_by" type="datetime" defaultValueComputed="null">
        <constraints nullable="true" unique="false"/>
      </column>
    </createTable>
    <insert
      tableName="company">
      <column name="id" value="1"/>
      <column name="name" value="National Council"/>
